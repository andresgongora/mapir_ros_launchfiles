  ### RVIZ ###
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find missions_pkg)/launch/andresgongora/2018_gsl_comparison/launch/gaden_simulator/gaden_simulator.rviz"/>

  
</launch>
