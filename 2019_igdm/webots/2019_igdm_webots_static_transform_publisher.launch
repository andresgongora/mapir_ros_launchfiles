<!-- publish tf map-odom -->

<launch>
	<node 
		pkg="tf2_ros" 
		type="static_transform_publisher" 
		name="tf_map_odom_broadcaster"
		respawn="true"
		output="screen"
		args="0 1 0 0 0 0 map odom"
	/>


	<node 
		pkg="tf2_ros" 
		type="static_transform_publisher" 
		name="tf_base_footprint_link_broadcaster"
		respawn="true"
		output="screen"
		args="0 0 0 0 0 0 base_footprint base_link"
	/>
</launch>
